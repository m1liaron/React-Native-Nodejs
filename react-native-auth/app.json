{
  "expo": {
    "name": "react-native-auth",
    "slug": "react-native-auth",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "14.0"
          },
          "android": {
            "compileSdkVersion": 33, 
            "targetSdkVersion": 33  
          }
        }
      ]
    ],
    "ios": {
      "bundleIdentifier": "com.m1liaron.reactnativeauth",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.13567678850-nf6hfemp1qi08t8l0kepo8ufl8bc8jq5"
            ]
          }
        ]
      }
    },
    "android": {
      "package": "com.m1liaron.reactnativeauth",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "31294db7-685f-42a5-9641-dbcd2a5ee82f"
      }
    },
    "owner": "m1liaron"
  }
}
